<footer role="contentinfo">
  <div class="moduk-footer
    {%- if params.classes %} {{ params.classes }}{% endif %}" id="moduk-footer"
    {%- for attribute, value in params.attributes %} {{ attribute }}="{{ value }}"{% endfor %}>
    <div class="moduk-width-container">
      {%- if params.links %}
      <h2 class="moduk-u-visually-hidden">Support links</h2>
      <ul class="moduk-footer__list">
        {%- for item in params.links %}
        <li class="moduk-footer__list-item"><a class="moduk-footer__list-item-link" href="{{ item.URL }}">{{ item.label }}</a></li>
        {%- endfor %}
      </ul>
      {% endif %}
      <p class="moduk-footer__copyright">{% if params.copyright %}{{ params.copyright | safe }}{% else %}&copy; Crown copyright{% endif %}</p>
    </div>
  </div>
</footer>
